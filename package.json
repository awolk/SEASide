{
  "name": "SEASide",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "decko": "^1.2.0",
    "electron-is-dev": "^0.3.0",
    "flow-bin": "^0.68.0",
    "keypair": "^1.0.1",
    "mkdirp": "^0.5.1",
    "node-forge": "^0.7.4",
    "react": "^16.2.0",
    "react-app-rewired": "^1.5.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "semantic-ui-css": "^2.3.0",
    "semantic-ui-react": "^0.78.3",
    "ssh2": "^0.6.0",
    "wait-on": "^2.1.0",
    "xterm": "^3.2.0"
  },
  "devDependencies": {
    "electron": "^1.8.3",
    "electron-builder": "^20.5.1",
    "react-addons-css-transition-group": "^15.6.2"
  },
  "scripts": {
    "react-start": "cross-env BROWSER=none react-app-rewired start",
    "react-build": "react-app-rewired build",
    "react-test": "react-app-rewired test --env=jsdom",
    "react-eject": "react-app-rewired eject",
    "electron-start": "electron .",
    "start": "concurrently 'yarn react-start' 'wait-on http://localhost:3000 && yarn electron-start'",
    "preelectron-build": "yarn react-build",
    "electron-build": "build"
  },
  "build": {
    "appId": "seaside",
    "productName": "SEASide",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "./",
  "main": "public/electron.js"
}
